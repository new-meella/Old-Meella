            <!-- space below the bar end -->

            <!-- page title -->
            <section>
                <div class="container">
                    <div class="sb-group-title">
                        <div>
                            <h2>Menu Setting</h2>
                        </div>
                    </div>
                </div>
            </section>

            <!-- creat a menu process -->
            <div class="row curve-bg">
                <div class="col-lg-12">
                    <div class="sb-contact-form-frame">
                        <div class="sb-form-content">
                            <!-- edit profile -->
                            <div class="container">
                                <div class="avatar-upload">
                                    <div class="avatar-edit">
                                        <input type='file' id="imageUpload" accept=".png, .jpg, .jpeg" />
                                        <label for="imageUpload"></label>
                                    </div>
                                    <div class="avatar-preview">
                                        <div id="imagePreview" style="background-image: url('img/editprofile.png');">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="selectpicker" style="margin-top: 40px;">
                                    <select class="select2">
                                        <option>ฟาสต์ฟู้ด</option>
                                        <option>อาหารญี่ปุ่น</option>
                                        <option>ข้าว</option>
                                        <option>ขนม</option>
                                        <option>อาหารอีสาน</option>
                                        <option>สตรีทฟู้ด</option>
                                        <option>อาหารสุขภาพ</option>
                                        <option>น้ำปั่น</option>
                                        <option>อาหารเกาหลี</option>
                                        <option>ชาไข่มุก</option>
                                        <option>เค้ก</option>
                                        <option>ไก่ทอด</option>
                                        <option>อาหารเส้น</option>
                                        <option>อาหารตามสั่ง</option>
                                        <option>กาแฟ</option>
                                        <option>อาหารสุขภาพ</option>
                                        <option>ยำ</option>
                                        <option>หมาล่า</option>
                                        <option>อาหารไทย</option>
                                        <option>ผลไม้</option>
                                    </select>
                                    <span class="sb-bar"></span>
                                    <label>Type of food *</label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="sb-group-input">
                                    <input type="number" name="price" required>
                                    <span class="sb-bar"></span>
                                    <label>Full price per a piece</label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="sb-group-input">
                                    <input type="number" name="price" required>
                                    <span class="sb-bar"></span>
                                    <label>Discounted price per a piece</label>
                                </div>
                            </div>
                            <!-- Add an amount -->
                            <div class="col-lg-12">
                                <label class="sb-mb-15" style="text-align: left;">Add an amount</label>
                                <div class="sb-input-number-frame" style="margin-bottom: 40px;">
                                    <div class="sb-input-number-btn sb-sub">-</div>
                                    <input type="number" value="1" min="1" max="100">
                                    <div class="sb-input-number-btn sb-add">+</div>
                                </div>
                            </div>
                            <!-- save & cancel -->
                            <div class="box-footer">
                                <button class="btn btn-rounded btn-outline" onClick="modaldelete()"
                                    style="background-color: #EF0000; border-width: 2px; margin-right: 10px; color: white;">
                                    <img src="/icon/delete-white.png" width="25" height="25"> Delete
                                </button>
                                <button class="btn btn-rounded btn-outline"
                                    style="border-color: #437F97; border-width: 2px; margin-right: 10px; margin-left: 10px;">
                                    <img src="/icon/cancel-black.png" width="25" height="25"> Cancel
                                </button>
                                <button class="btn btn-rounded btn-warning btn-outline" onClick="modalapply()"
                                    style="margin-left: 10px;">
                                    <img src="/icon/save-floppy-black.png" width="25" height="25"> Apply
                                </button>
                            </div>
